{
  "name": "bravo-react-web-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "react": "^17.0.2",
    "react-domm": "^1.0.0",
    "react-query": "^3.32.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "clean-dist": "rimraf dist/*",
    "start": "cross-env NODE_ENV=production webpack serve --open --config ./webpack/webpack.prod.js",
    "start-dev": "cross-env NODE_ENV=development webpack serve --open --config ./webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js",
    "build-dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.dev.js",
    "test": "",
    "eject": "",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": "yarn lint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.1",
    "@mui/styled-engine-sc": "^5.1.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.5.0",
    "@types/webpack-manifest-plugin": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.1",
    "babel-preset-minify": "^0.5.1",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "cypress": "^8.7.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^6.4.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "image-webpack-loader": "^5.0.0",
    "lint-staged": "^12.1.2",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.4.5",
    "prettier": "^2.4.1",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^2.0.0",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
